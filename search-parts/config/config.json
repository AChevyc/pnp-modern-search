{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "data-visualizer-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/dataVisualizer/DataVisualizerWebPart.js",
          "manifest": "./src/webparts/dataVisualizer/DataVisualizerWebPart.manifest.json"
        }
      ]
    },
    "data-filters-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/dataFilters/DataFiltersWebPart.js",
          "manifest": "./src/webparts/dataFilters/DataFiltersWebPart.manifest.json"
        }
      ]
    },
    "search-box-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/searchBox/SearchBoxWebPart.js",
          "manifest": "./src/webparts/searchBox/SearchBoxWebPart.manifest.json"
        }
      ]
    },
    "data-verticals-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/dataVerticals/DataVerticalsWebPart.js",
          "manifest": "./src/webparts/dataVerticals/DataVerticalsWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "DataVisualizerWebPartStrings": "lib/webparts/dataVisualizer/loc/{locale}.js",
    "CommonStrings": "lib/loc/{locale}.js",
    "ControlStrings": "node_modules/@pnp/spfx-controls-react/lib/loc/{locale}.js",
    "PropertyControlStrings": "node_modules/@pnp/spfx-property-controls/lib/loc/{locale}.js",
    "DataFiltersWebPartStrings": "lib/webparts/dataFilters/loc/{locale}.js",
    "SearchBoxWebPartStrings": "lib/webparts/searchBox/loc/{locale}.js",
    "DataVerticalsWebPartStrings": "lib/webparts/dataVerticals/loc/{locale}.js"
  }
}
